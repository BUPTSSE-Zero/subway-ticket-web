<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">
    <persistence-unit name="SubwayTicketDBPU" transaction-type="JTA">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <jta-data-source>jdbc/subwayticketdb</jta-data-source>
        <class>com.subwayticket.database.model.Account</class>
        <class>com.subwayticket.database.model.SystemMessage</class>
        <class>com.subwayticket.database.model.City</class>
        <class>com.subwayticket.database.model.SubwayLine</class>
        <class>com.subwayticket.database.model.StationMessage</class>
        <class>com.subwayticket.database.model.SubwayStation</class>
        <class>com.subwayticket.database.model.TicketPrice</class>
        <class>com.subwayticket.database.model.TicketOrder</class>
    </persistence-unit>
</persistence>
